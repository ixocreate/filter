{
    "name": "ixocreate/filter",
    "description": "Filter of IXOCREATE",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/contract": "=1.0.0-alpha1"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0",
        "phpunit/phpunit": "^6.0.6",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateTest\\Filter\\": "tests/src/",
            "IxocreateMisc\\Filter\\": "tests/misc/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
